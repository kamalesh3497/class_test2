#include<stdio.h>
int main()
{
	int a[10]={1,2,3,4,5,6,7,8,9,0};
	int b;
	for (int i=0;i<10;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("enter the number");
	scanf("%d",&b);
	for(int i=0;i<10;i++)
	{
	if(a[i]==b)
	{
		printf("given number is present\n");
		printf("index value : %d \n",i);}
	}
	return 0;
}
