#include<stdio.h>
#include<string.h>
int main()
{
	char str1[100],str2[100];
	printf("enter the str1\n");
	scanf("%s",str1);
	printf("enter the str2\n");
	scanf("%s",str2);
	int result=strcmp(str2,str1);
	if(result==0)
	printf("both are equal %d",result);
	else
	printf("not equal %d",result);
	return 0;
}
